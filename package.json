{
  "name": "database-services",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc && npx copyfiles -u 1 src/config/config.js dist/",
    "start": "yarn run build && node dist/index.js",
    "migrate": "yarn sequelize-cli db:migrate --config=src/config/config.js",
    "seed": "yarn sequelize-cli db:seed:all"
  },
  "dependencies": {
    "@tsconfig/node16": "^16.1.3",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "8.5.1",
    "nanoid": "3",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.5",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/node": "^22.13.1",
    "@types/sequelize": "^4.28.20",
    "copyfiles": "^2.4.1",
    "cpy-cli": "^5.0.0",
    "nodemon": "^3.1.9",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
